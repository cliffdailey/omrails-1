$boxes = $('<%= j render(@pins) %>');
$('#pins').append( $boxes ).imagesLoaded( function(){
	$('#pins').masonry( 'reload');
});
<% if (@pins.current_page < @pins.num_pages) %>	
	setTimeout(function(){
		$('.pagination').replaceWith('<%= j paginate(@pins) %>');
	},1000);						
<% else %>
	$('.pagination').remove();
<% end %>

